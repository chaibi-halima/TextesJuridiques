                <option>2017</option>
                <option>2016</option>
                <option>2015</option>
                <option>2014</option>
                <option>2013</option>
            </select>
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="form-group">
            <label>N° du JORT</label>
            <input type="text" name="" id=""  class="form-control" placeholder="N° du JORT">
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-3">
        <div class="form-group">
            <label>Date du JORT</label>
            <input type="date" class="form-control" placeholder="Date">
        </div>
    </div>
</div>
