{% block stylesheet %}
    <link href="{{asset('bower_components/bootstrap/dist/css/bootstrap.css')}}" rel="stylesheet">
{% endblock %} 

{{ form_errors(form) }}
<div class="col-lg-12 col-md-12 col-sm-12">
    <div class="col-lg-3 col-md-3 col-sm-6">
         {{ form_errors(form.typetexte) }}
        <div class="form-group">
            <label>Type du Texte</label>
             {{ form_widget(form.typetexte, { 'attr': {'class': 'form-control','placeholder': 'Entrer Nature'} }) }}
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6">
        {{ form_errors(form.annee) }}
        <div class="form-group">
            <label>Année du texte</label>
             {{ form_widget(form.annee, { 'attr': {'class': 'form-control','placeholder': 'Entrer Annee'} }) }}
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6">
        <div class="form-group">
            <label>N° du texte</label>
            <input type="text" name="" id=""  class="form-control" placeholder="N° du texte">
        </div>
    </div>
    <div class="col-lg-3 col-md-3 col-sm-6">
        <div class="form-group">
            <label>Date du texte</label>
            <input type="date" class="form-control" placeholder="Date">
        </div>
    </div>
</div>
<div class="col-lg-12 col-md-12 col-sm-12">
    <div class="col-lg-3 col-md-3 col-sm-6">
